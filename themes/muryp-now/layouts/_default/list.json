[
{{ range $index, $page := .Pages }}
{{- if ne $page.Type "json" -}}
{{- if and $index (gt $index 0) -}},{{- end }}
{
	"link": "{{ $page.Permalink }}",
	"judul": "{{ htmlEscape $page.Title}}",
	"description": "{{ htmlEscape .Description}}",
{{ with .Params.tags }}
"tags":"{{range .}} #{{ replace . " " "_"  }}{{end}}"
  {{end}}
}
{{- end -}}
{{- end -}}
]
